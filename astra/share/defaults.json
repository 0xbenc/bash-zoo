{
  "ui": {
    "layout": "fzf",
    "theme": "default"
  },
  "browser": {
    "show_hidden": false,
    "sort": {
      "mode": "name",
      "reverse": false
    }
  },
  "history": {
    "limit": 200
  },
  "session": {
    "resume_last": false
  },
  "actions": {
    "delete": {
      "use_trash": true
    }
  },
  "preview": {
    "max_bytes": 1048576
  },
  "plugins": {
    "enabled": true,
    "dir": "~/.config/astra/plugins"
  }
}
